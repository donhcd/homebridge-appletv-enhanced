{
    "watch": [
        "src",
        "user_storage/config.json",
        "requirements.txt",
        "requirements_openssl_legacy.txt"
    ],
    "ext": "ts",
    "ignore": [],
    "exec": "tsc && homebridge --insecure --debug --user-storage-path ./user_storage",
    "signal": "SIGTERM",
    "env": {
        "NODE_OPTIONS": "--trace-warnings"
    }
}
